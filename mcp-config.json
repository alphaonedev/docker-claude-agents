{
  "mcpServers": {
    "github": {
      "command": "mcp-server-github",
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "set-via-docker-compose-env"
      }
    },
    "filesystem": {
      "command": "mcp-server-filesystem",
      "args": ["/app/workspace"]
    },
    "brave-search": {
      "command": "mcp-server-brave-search",
      "env": {
        "BRAVE_API_KEY": "set-via-docker-compose-env"
      }
    },
    "postgres": {
      "command": "mcp-server-postgres",
      "env": {
        "DATABASE_URL": "set-via-docker-compose-env"
      }
    }
  }
}
